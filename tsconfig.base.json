      ],
      "@joke/web/my-jokes/ui/jokes-view": [
        "libs/web/my-jokes/ui/jokes-view/src/index.ts"
      ],
      "@joke/web/shared/ui/joke-page-view-base": [
        "libs/web/shared/ui/joke-page-view-base/src/index.ts"
      ],
      "@joke/web/ui/nav": ["libs/web/ui/nav/src/index.ts"],
      "@joke/web-shared-ui-joke-card": ["libs/web/shared/ui/joke-card/src/index.ts"]
