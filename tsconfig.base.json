      ],
      "@joke/web-shared-data-access-jokes": [
        "libs/web/shared/data-access-jokes/src/index.ts"
      ],
      "@joke/web-shared-domain-types": [
        "libs/web/shared/domain-types/src/index.ts"
      ],
      "@joke/web-shared-feature-joke-add-dialog": [
        "libs/web/shared/feature-joke-add-dialog/src/index.ts"
      ],
      "@joke/web-shared-ui-snackbar": [
        "libs/web/shared/ui/snackbar/src/index.ts"
      ],
      "@joke/web-shell": ["libs/web/shell/src/index.ts"],
      "@joke/web-ui-styles": ["libs/web/ui/styles/src/index.ts"],
